{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-optional-chaining",
    [
      "@babel/plugin-proposal-decorators",
      {
        "version": "2023-05"
      }
    ],
    "@babel/plugin-proposal-private-methods",
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}